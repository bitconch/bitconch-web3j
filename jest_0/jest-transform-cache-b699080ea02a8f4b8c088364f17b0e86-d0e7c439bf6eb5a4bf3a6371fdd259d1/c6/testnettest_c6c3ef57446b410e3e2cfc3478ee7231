"use strict";

var _testnet = require("../src/util/testnet");

test('invalid', function () {
  expect(function () {
    (0, _testnet.testnetChannelEndpoint)('abc123');
  }).toThrow();
});
test('edge', function () {
  expect((0, _testnet.testnetChannelEndpoint)('edge')).toEqual('https://api.edge.testnet.bitconch.com');
});
test('default', function () {
  (0, _testnet.testnetChannelEndpoint)(); // Should not throw
});
