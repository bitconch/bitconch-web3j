"use strict";

var _src = require("../src");

test('createNewAccount', function () {
  var from = new _src.BusAccount();
  var createNewAccount = new _src.BusAccount();
  var transaction;
  transaction = _src.SystemController.createNewAccount(from.pubKey, createNewAccount.pubKey, 123, 0, _src.BudgetController.space, _src.BudgetController.controllerId);
  expect(transaction.keys).toHaveLength(2);
  expect(transaction.controllerId).toEqual(_src.SystemController.controllerId); // TODO: Validate transaction contents more
});
test('transfer', function () {
  var from = new _src.BusAccount();
  var to = new _src.BusAccount();
  var transaction;
  transaction = _src.SystemController.transfer(from.pubKey, to.pubKey, 123);
  expect(transaction.keys).toHaveLength(2);
  expect(transaction.controllerId).toEqual(_src.SystemController.controllerId); // TODO: Validate transaction contents more
});
test('transferReputation', function () {
  var from = new _src.BusAccount();
  var to = new _src.BusAccount();
  var transaction;
  transaction = _src.SystemController.transferReputation(from.pubKey, to.pubKey, 123);
  expect(transaction.keys).toHaveLength(2);
  expect(transaction.controllerId).toEqual(_src.SystemController.controllerId); // TODO: Validate transaction contents more
});
test('assign', function () {
  var from = new _src.BusAccount();
  var to = new _src.BusAccount();
  var transaction;
  transaction = _src.SystemController.assign(from.pubKey, to.pubKey);
  expect(transaction.keys).toHaveLength(1);
  expect(transaction.controllerId).toEqual(_src.SystemController.controllerId); // TODO: Validate transaction contents more
});
